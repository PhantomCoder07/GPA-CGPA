#include <stdio.h>
int main()
{
    float a;
    printf ("Enter the Score: ");
    scanf ("%f",&a);
    if (a>=80 && a<=100)
        printf ("Obtained GPA: A+");
    else if (a>=70 && a<=79)
        printf ("Obtained GPA: A");
    else if (a>=60 && a<=69)
        printf ("Obtained GPA: A-");
    else if (a>=50 && a<=59)
        printf ("Obtained GPA: B");
    else if (a>=40 && a<=49)
        printf ("Obtained GPA: C");
    else if (a>=33 && a<=39)
        printf ("Obtained GPA: D");
    else if (a>=0 && a<=32)
        printf ("Obtained GPA: F");
    else
        printf ("Invalid Score.");
    return 0;
}
