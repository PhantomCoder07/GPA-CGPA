#include <stdio.h>
int main()
{
    float a;
    printf ("Enter the Score: ");
    scanf ("%f",&a);
    (a>=80 && a<=100)?(printf ("Obtained GPA: A+")):
        ((a>=70 && a<=79)?(printf("Obtained GPA: A")):
         (a>=60 && a<=69)?(printf("Obtained GPA: A-")):
         (a>=50 && a<=59)?(printf("Obtained GPA: B")):
         (a>=40 && a<=49)?(printf("Obtained GPA: C")):
         (a>=33 && a<=39)?(printf("Obtained GPA: D")):
         (a>=0 && a<=32)?(printf("Obtained GPA: F")):
         (printf("Invalid Score")));
    return 0;
}
